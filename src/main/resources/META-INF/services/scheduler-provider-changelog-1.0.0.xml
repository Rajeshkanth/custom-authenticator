<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="Easy Trasnfer" id="scheduler-provider-1.0.0">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="SCHEDULER_PROVIDER"/>
            </not>
        </preConditions>
        <createTable tableName="SCHEDULER_PROVIDER">

            <column name="ID" type="VARCHAR(36)">
                <constraints nullable="false"/>
            </column>
            <column name="ALIAS" type="VARCHAR(250)"/>
            <column name="NAME" type="VARCHAR(250)"/>
            <column name="REALM_ID" type="VARCHAR(250)"/>
            <column name="PROVIDER_ID" type="VARCHAR(250)"/>
            <column name="INTRVL" type="INT"/>
            <column name="CREATE_TIME" type="BIGINT"/>
            <column name="LAST_RUN_TIME" type="BIGINT"/>
            <column name="NEXT_RUN_TIME" type="BIGINT"/>
            <column name="SETTINGS" type="VARCHAR(250)"/>
        </createTable>
        <addPrimaryKey
                constraintName="SCHEDULER"
                tableName="SCHEDULER_PROVIDER"
                columnNames="ID"
        />
    </changeSet>
</databaseChangeLog>